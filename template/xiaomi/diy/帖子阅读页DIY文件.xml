					<item id="template">
						<item id="raw"><![CDATA[<div class="bm xm_bm xm_bm_gal" id="xm_gallery">
    <div class="bm_c">
        <ul>
[loop]
<li>
<a href="{url}"{target}>
<img src="{pic}" width="{picwidth}" height="{picheight}" /><span>{title}</span></a>
</li>
[/loop]
</ul>
        <div class="toggle" id="xm_gal_toggle"></div>

        <script type="text/javascript">
            var obj = document.getElementById("xm_gallery");

            var pics = obj.getElementsByTagName("li");
            var pic_num = pics.length;
            var pic_cur = 0;

            var timer;

            var toggles = document.getElementById("xm_gal_toggle");

            var showPic = function (id) {
                var temp = toggles.getElementsByTagName("a");
                for (var i in temp) {
                    temp[i].className = temp[i].id === "tog_" + id?"current":"";
                }

                for (var i in pics) {
                    if (pics[i].tagName && pics[i].tagName.toLocaleUpperCase() === "LI") {
                        pics[i].style.display = (pics[i].id === "pic_" + id)?"block":"none";
                    }
                }

                return false
            }

            var rollPic = function () {
                clearTimeout(timer);

                if (pic_cur >= pic_num) {
                    pic_cur = 0;
                }

                showPic(pic_cur);

                pic_cur++;

                timer = setTimeout(rollPic, 5000);

            }

            for (var i in pics) {
                if (pics[i].tagName && pics[i].tagName.toUpperCase() === "LI") {
                    pics[i].id = "pic_" + i;

                    var toggle = document.createElement("a");
                    toggle.setAttribute("href", "#");
                    toggle.id = "tog_" + i;
                    toggle.setAttribute("onclick", "pic_cur = " + i + "; rollPic(); return false");

                    toggles.appendChild(toggle);
                }
            }

            if (pic_num > 0) {
                rollPic();

                obj.onmouseover = function() {clearTimeout(timer)};
                obj.onmouseleave = function() {timer = setTimeout(rollPic, 5000)};
            }

        </script>
    </div>
</div>]]></item>
